<?xml version="1.0" encoding="UTF-8"?>
<MinecartManiaConfiguration>
	<version>1.02</version>
	<!--
	Keys are mapped to specific locations in MM's code. Do not add or remove keys unless you know what you are doing.
	The text is the text that is displayed to the user. You are free to change the text to anything to suit your needs.
	Note: %s, %d, and %f are variable formatting characters. Look up their usage online if you are unsure.

	Colors: 
	
	[WHITE]
	[BLACK]
	[DARK_BLUE]
	[DARK_GREEN]
	[DARK_AQUA]
	[DARK_PURPLE]
	[GOLD]
	[GRAY]
	[DARK_GRAY]
	[BLUE]
	[GREEN]
	[AQUA]
	[RED]
	[LIGHT_PURPLE]
	[YELLOW]
	
	-->
	<TextKeys>
		<TextKey>
			<Key>AutoCartEnteredMessage</Key>
			<Text>[GREEN]Tapez le wagonnet dans la direction où vous voulez aller</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsValidDebugModes</Key>
			<Text>[RED]Modes débug valides : %s</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsDebugMode</Key>
			<Text>[GREEN]Passé en débug : %s</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsNoPlayerFound</Key>
			<Text>[RED]Aucun joueur de ce nom</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsEjectUsage</Key>
			<Text>[RED]Utilisation : '/eject [nom joueur]'</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsPermEjectUsage</Key>
			<Text>[RED]Utilisation : '/permeject [nom joueur]'</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsMinecartsRemoved</Key>
			<Text>[GREEN]%d wagonnets ont été retirés du serveur.</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsInvalidValue</Key>
			<Text>[RED]Valeur de configuration invalide</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsSetConfigKeyUsage</Key>
			<Text>[RED]Utilisation : '/setconfigkey [clé] [valeur]'. \nAstuce : La clé et la valeur devraient être entre crochets.</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsMomentum</Key>
			<Text>Vitesse X: %.2f \nVitesse Y: %.2f \nVitesse Z: %.2f</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsMomentumInvalid</Key>
			<Text>[RED]Vous ne pouvez avoir de vitesse qu'à l'intérieur d'un wagonnet.</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsStation</Key>
			<Text>[GREEN]Type de station réglé à : %s</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsConfigKey</Key>
			<Text>La valeur de '%s' est '%s'</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsConfigKeyUsage</Key>
			<Text>[RED]Utilisation : '/getconfigkey [clé]'. \nHint: La clé devrait être entre crochets.</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsListConfigKeyPageHeader</Key>
			<Text>Valeurs de configuration Minecart Mania (Page %d sur %d)</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsListConfigKey</Key>
			<Text>Clé : [GREEN]%s [WHITE]Valeur : [GREEN]%s</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsSetConfigKey</Key>
			<Text>Clé '[GREEN]%s[WHITE]' réglée à '[GREEN]%s[WHITE]'</Text>
		</TextKey>
		<TextKey>
			<Key>AdminControlsListConfigKeyPage</Key>
			<Text>[RED]Il n'y a pas de page %d</Text>
		</TextKey>
